  "dependencies": {
    "@bfchain/util-platform": "4.12.44",
    "@bfchain/util-typings": "4.12.44"
  },
  "exports": {
    ".": {
      "require": "./build/cjs/index.js",
      "import": "./build/esm/index.js",
      "types": "./build/esm/index.d.ts"
    },
    "./text": {
      "node": {
        "require": "./build/cjs/text.node.js",
        "import": "./build/esm/text.node.js",
        "types": "./build/esm/text.node.d.ts"
      },
      "default": {
        "require": "./build/cjs/text.browser.js",
        "import": "./build/esm/text.browser.js",
        "types": "./build/esm/text.browser.d.ts"
      }
    }
  },
  "typesVersions": {
    "*": {
      "text": [
        "./build/esm/text.node.d.ts",
        "./build/esm/text.browser.d.ts"
      ]
    }
  },
  "types": "build/cjs/index.d.ts"
